{
  "luci-app-nodemanager": {
    "description": "Grant LuCI Node Manager permissions",
    "read": {
      "uci": [
        "nodemanager"
      ],
      "file": {
        "/etc/nikki/*": [
          "read"
        ],
        "/usr/share/nodemanager/*": [
          "read"
        ]
      }
    },
    "write": {
      "uci": [
        "nodemanager"
      ],
      "file": {
        "/etc/nikki/*": [
          "write"
        ]
      }
    }
  }
}